<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="d" class="com.nayun.jul022.dog.Dog"></bean>
	
	<bean id="d2" class="com.nayun.jul022.dog.Dog">
		<property name="name" value="개님"></property>
		<property name="age" value="99"></property>
	</bean>
	<bean id="d3" class="com.nayun.jul022.dog.Dog">
		<constructor-arg value="개느님"></constructor-arg>
		<constructor-arg value="999"></constructor-arg>
	</bean>
	
	<bean id="book" class="com.nayun.jul022.book.Book"></bean>
	
	<bean id="book2" class="com.nayun.jul022.book.Book">
		<property name="name" value="미움받을용기"></property>
		<property name="price" value="18000"></property>
	</bean>
	<bean id="book3" class="com.nayun.jul022.book.Book">
		<constructor-arg value="아비투스"></constructor-arg>
		<constructor-arg value="19800"></constructor-arg>
	</bean>
</beans>
